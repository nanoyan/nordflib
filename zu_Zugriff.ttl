	skos:topConceptOf n0:;
	skos:prefLabel "Gastwissenschaftlerarbeitsplatz"@de; 
	skos:definition "On-Site-Nutzung – Keine Übermittlung der Daten zum*r Nutzer*in"@de.
n1:6
	 a skos:Concept;
